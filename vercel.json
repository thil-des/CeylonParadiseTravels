{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/(.*)\\.js", "dest": "/$1.js" },
    { "src": "/(.*)\\.css", "dest": "/$1.css" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
